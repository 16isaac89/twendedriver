        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDSGXiLg9kRk_93B-s_2VFkrnqHfULeZtI"
        }
      },
      "package": "com.twendedeliveries.driver",
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"]
      
     
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c59fe50b-c951-45da-9f02-6bc0fa21fd40"
      }
    }
